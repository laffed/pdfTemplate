doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet" type="text/css" href='../styles/fema_pg1.css')
    title FEMA pg. 1

  body
    div(class="page")
      div(class="gridBoxHeader")
        div(class="gridBoxHeader-col1")
          p(class="gridBoxHeader-col1_headingSmall") DEPARTMENT OF HOMELAND SECURITY
          p(class="gridBoxHeader-col1__headingNormal") Federal Emergency Management Agency
          p(class="gridBoxHeader-col1-headingBold") ADJUSTER'S PRELIMINARY REPORT

        div(class="gridBoxHeader-col2")
          p OMB Control Number: 1660-0005
          p Expiration: 7-31-2020
        
      div(class="gridFirstBox")
        div(class="gridFirstBox-paragraph")
          p 
            | NOTE: The NFIP requires that a preliminary report be received within 
            span(class="bold_text") 15 days 
            |of assignment.
        
        div(class="gridFirstBox__leftColumn")
          div(class="gridFirstBox__leftColumn-row1")
            p NAME(S) OF INSURED:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row2")
            p Property Address:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row3")
            div(class="gridFirstBox__leftColumn-row3-col")
              p City:
              input(type="text" placeholder="Dummy text")

            div(class="gridFirstBox__leftColumn-row3-col")
              p State:
              input(type="text" placeholder="Dummy text")

            div(class="gridFirstBox__leftColumn-row3-col")
              p ZIP:
              input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row4")
            p Mailing/Temporary Address:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row5")
            p City:
            input(type="text" placeholder="Dummy text")
            p State:
            input(type="text" placeholder="Dummy text")
            p ZIP:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row6")
            p Best Contact Number:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row7")
            p Alternate Contact Number:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row8")
            p Adjusting Company:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row9")
            p Adjuster Address:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row10")
            p City:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row11")
            p State:
            input(type="text" placeholder="Dummy text")
            p ZIP:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row12")
            p Adjuster's Telephone Numbers:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__leftColumn-row13")
            p Work:
            input(type="text" placeholder="Dummy text")
            p Mobile:
            input(type="text" placeholder="Dummy text")


        div(class="gridFirstBox__rightColumn")
          div(class="gridFirstBox__rightColumn-row1")
            p POLICY NUMBER:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__rightColumn-row2")
            p Date of Loss:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__rightColumn-row3")
            p FICO Number:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__rightColumn-row4")
            p Adjuster's File Number:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__rightColumn-row5")
            p Tax ID Number:
            input(type="text" placeholder="Dummy text")
    
          div(class="gridFirstBox__rightColumn-row6 marginTopHelper")
            p Date Loss Assigned:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__rightColumn-row7")
            p Date Insured Contacted:
            input(type="text" placeholder="Dummy text")

          div(class="gridFirstBox__rightColumn-row8")
            p Date Loss Inspected:
            input(type="text" placeholder="Dummy text")

      div(class="gridSecondBox")
        div(class="gridSecondBox__leftColumn text_rotate bold_text") ATTS.
      
        div(class="gridSecondBox__rightColumn")
          div(class="gridSecondBox__rightColumn-row1")
            p Attachments (enter number of each inside parentheses)

          div(class="gridSecondBox__rightColumn-row2")
            div
              input(type="checkbox")

              p Building worksheets
              span 
                |(
                input(type="text")
                |)
            div
              input(type="checkbox")
              p Photographs
              span 
                |(
                input(type="text")
                |)

            div
              input(type="checkbox")
              p Proof of loss

            div
              input(type="checkbox")
              p Other

            div  
              p (specify)
              input(type="text")

          div(class="gridSecondBox__rightColumn-row3")
            div
              input(type="checkbox")
              p Contents worksheets
              span 
                |(
                input(type="text")
                |)

            div
              input(type="checkbox")
              p Narrative
              span 
                  |(
                  input(type="text")
                  |  pages)

            div
              input(type="checkbox")
              p R/C Proof
            
            div
              input(type="checkbox")
              p Other
            
            div
              p (specify)
              input(type="text")

      div(class="gridThirdBox")
        div(class="gridThirdBox__leftColumn text_rotate bold_text") INSURANCE

        div(class="gridThirdBox__middleColumn")
          div(class="gridThirdBox__middleColumn-left")
            p Coverage Verified From:
            div
              input(type="checkbox")
              p NFIP
            div
              input(type="checkbox")
              p Agent's Daily
            div
              input(type="checkbox")
              p Insured's Policy
          
          div(class="gridThirdBox__middleColumn-right")
            p Policy Term
            div
              p From:
              input(type="text")
            
            div
              p To:
              input(type="text")
        
        div(class="gridThirdBox__rightColumn")
          div(class="gridThirdBox__rightColumn-left")
            p Program:
            div
              input(type="checkbox")
              p Emergency
            div
              input(type="checkbox")
              p Regular

          div(class="gridThirdBox__rightColumn-right")
            p SFIP Form:
            div
              input(type="checkbox")
              p General Property
            div
              input(type="checkbox")
              p Dwelling
            div
              input(type="checkbox")
              p RCBAP


        div(class="gridThirdBox__row2-leftColumn")
          div(class="inline-flex")
            p Advance payment requested?
            input(type="checkbox")
            span Yes
            input(type="checkbox")
            span No

          em If yes, submit Proof of Loss for (FF 086-0-9) for amount of payment and supporting documentation with this report.

        div(class="gridThirdBox__row2-rightColumn")

          div
            p Building
            p Contents

          div
            p COVERAGE
            span
              |$
              input(type="text")
            span
              |$
              input(type="text")

          div
            p DEDUCTIBLE
            span
              |$
              input(type="text")
            span
              |$
              input(type="text")

          div
            p RESERVE
            span
              |$
              input(type="text")
            span
              |$
              input(type="text")
      
      div(class="gridFourthBox")
        div(class="gridFourthBox__leftColumn text_rotate bold_text") RISK

        div(class="gridFourthBox__rightColumn")
          div(class="gridFourthBox__rightColumn-Grid")
            div(class="gridFourthBox__rightColumn-Grid--left")
              p TYPE OF BUILDING:

            div(class="gridFourthBox__rightColumn-Grid--right")

              div(class="gridFourthBox__rightColumn-Grid--right__FLEX")
                div
                  input(type="checkbox")
                  p Single Family
                div
                  input(type="checkbox")
                  p 2-4 Family
                div
                  input(type="checkbox")
                  p Condo Association
                div
                  input(type="checkbox")
                  p Condo Unit
                div
                  input(type="checkbox")
                  p Other Residential

              div
                input(type="checkbox")
                p Non-Residential (including Business Buildings and Other Non-Residential Buildings)

              div(class="gridFourthBox__rightColumn-Grid--right__FLEX")
                div
                  input(type="checkbox")
                  p Mobile Home/Traveler Trailer:

                div
                  p Make:
                  input(type="text" placeholder="Dummy text")

              div(class="gridFourthBox__rightColumn-Grid--right__FLEX")
                div
                  p Model:
                  input(type="text" placeholder="Dummy text")

                div
                  p Serial Number:
                  input(type="text" placeholder="Dummy text")

          div(class="gridFourthBox2__rightColumn-Grid")
            div
              p OCCUPANCY:

              span
                input(type="checkbox")
                p Owner

              span
                input(type="checkbox")
                p Tenant

              span
                input(type="checkbox")
                p State Government owned

              span
                input(type="checkbox")
                p Unoccupied

              p RESIDENCY:
              
              span
                input(type="checkbox")
                p Principal

              span
                input(type="checkbox")
                p Seasonal
            
            div
              p TITLE VERIFIED?

              span
                input(type="checkbox")
                p Yes
              
              span
                input(type="checkbox")
                p No
              
              span
                p Source of verification:
                input(type="text" placeholder="Dummy Text")

          div(class="gridFourthBox3__rightColumn-Grid")
            div
              p Number of floors in the building including basement/crawl space:
              input(type="checkbox")
              p 1
              input(type="checkbox")
              p 2
              input(type="checkbox")
              p 3 (or more)
            
            div
              p Is building a split level?

              input(type="checkbox")
              p Yes
              input(type="checkbox")
              p No

            div
              p In case of multiple occupancy, indicate floor(s) occupied by insured:
             
              input(type="checkbox")
              p Basement

              input(type="checkbox")
              p First

              input(type="checkbox")
              p Second and/or above

          div(class="gridFourthBox4__rightColumn-Grid")
            div
              p Type of basement:

              input(type="checkbox")
              p None

              input(type="checkbox")
              p Unfinished

              input(type="checkbox")
              p Finished

              p Is basement flood-proofed?

              input(type="checkbox")
              p Yes
              
              input(type="checkbox")
              p No

          div(class="gridFourthBox4__rightColumn-Grid")
            div
              p Building elevated?
              
              input(type="checkbox")
              p Yes
              
              input(type="checkbox")
              p No
              
              input(type="checkbox")
              p Foundation area enclosure?
              
              input(type="checkbox")
              p None
              
              input(type="checkbox")
              p Breakaway walls
              
              input(type="checkbox")
              p Unfinished
              
              input(type="checkbox")
              p Finished